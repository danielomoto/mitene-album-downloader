{
  "name": "Mitene Album Downloader",
  "version": "1.1",
  "manifest_version": 3,
  "description": "Downloads all the images and videos from a Mitene album.",
  "permissions": [
    "scripting",
    "activeTab",
	"downloads",
	"storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}